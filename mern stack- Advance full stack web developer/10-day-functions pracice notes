//date
 
const f=new Date();
console.log(f);
console.log(f.getDate());
console.log(f.getDay());
console.log(f.getMonth());
console.log(f.getFullYear());
console.log(f.getHours());
console.log(f.getMinutes());
console.log(f.getSeconds());

