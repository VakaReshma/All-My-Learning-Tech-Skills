function isEvenOdd(a){
    if(a%2==0){
        console.log("Even");
    }
    else{
        console.log("Odd");
    }
}

isEvenOdd(45);
//return type function

function isEvenorOdd(b){
    if(b%2==0){
        return "Even";
    }
    else{
        return "Odd";
    }
}

const c =isEvenorOdd(52);
console.log(c)
